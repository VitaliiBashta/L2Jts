{
	# Новая система петиций
	# Должно быть включено в клиенте
	# l2.ini
	# [URL]
	# UseNewPetition" : "true",
	# Настройка через data/petition_group.xml
	"NewPetitionSystem" : "false",

	# Иной вариант миниигры
	# Должно быть включено в клиенте
	# l2.ini
	# [Localize]
	# UseJapanMinigame1" : "true",
	# Основное различие, это сохранения счета + показ найлучшего счета
	"JapanMinigame" : "false",

	# Система упрощего поиска помощи
	# Начальные игроки имеют флажек, дивимый всеми(если /mark on), включить флажек(/initial on)
	# После 6 месяцев, начальные игроки могут помогать другим, значек будет рукопожатия(/evangelist on)
	# l2.ini
	# [Localize]
	# UseLectureMark" : "true",
	"LectureMark" : "false",

	# Использовать при сохраннения телепорт вкладки - итем 20033(My Teleport Flag)
	# l2.ini
	# [Localize]
	# UseTeleportFlag" : "true",
	"UseTeleportFlag" : "true",

	# Включить получение информации по языку, от клиента. Так же устанавлиевает
	# дефолтное значение языка из localization.ini клиента. Для каждого игрока индивидуально!
	# Localization.ini
	# [LanguageSet]
	# Language" : "1",
	"ClientLanguageCheck" : "false",

	# Использовать ли авторизация по цыфровом пароле при выборе чара - должно быть включено в клиенте
	# Что б спрашивало пароль, нужно поставить password_enable в character_access
	# l2.ini
	# [Auth]
	# UseSecondaryAuth" : "true",
	"2ndPasswordCheck" : "false",

	# Система бот хантинга
	# Полный аналог системы с офа
	"EnableBotReporting" : "true",
	# Дефолтное количество доступных голосов
	"DefaultReportPoints" : "10",

	# Разрешить смену имени чара/клана если будет неверное
	# Использует при мерге или смене шаблона имени
	"ChangeNameDialogIfIncorrect" : "false",
	
	# По умолчанию "false" //TODO: сделать
	"GoodsInventoryEnable" : "false"
}