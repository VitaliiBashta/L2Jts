{
	# =================================================================
	# Настройки Community board
	# =================================================================
	# Включить коммунити борд?
	"AllowCommunityBoard" : "true",

	# Начальная страница коммунити?
	"BBSDefault" : "_bbshome",
	
	# Путь к папке с Community. data/html/Path - community
	"Path" : "community",
	
	# Время показанное в коммунити (указывается часовой пояс, можно найти https://en.wikipedia.org/wiki/List_of_tz_database_time_zones)
	"TimeZone" : "Europe/Moscow",
	
	# Умножение онлайнa играков показанного в коммунити(1. - дефолт, можно умножать на 1.2, 1.3 - 2)
	"IncreaseOnline" : "6.",
	
	# Умножение оффлайн играков показанного в коммунити(1. - дефолт, можно умножать на 1.2, 1.3 - 2)
	"IncreaseOffline" : "1."
}